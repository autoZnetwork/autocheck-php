{
    "name": "autoznetwork/autocheck-php",
    "description": "",
    "keywords": [
        "autoznetwork",
        "autocheck-php"
    ],
    "homepage": "https://github.com/autoZnetwork/autocheck-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Collin Pedersen",
            "email": "collin@autoznetwork.com",
            "homepage": "https://www.autoznetwork.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "kamermans/guzzle-oauth2-subscriber": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Autoznetwork\\AutoCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autoznetwork\\AutoCheck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
